{
  "name": "redux-grid",
  "version": "0.1.8",
  "description": "Grid component built for Redux.",
  "keywords": [
    "redux",
    "react",
    "grid",
    "tables",
    "charts",
    "charting",
    "pagination",
    "sort"
  ],
  "main": "lib/index.js",
  "scripts": {
    "link-dev": "npm link ../redux-grid-view",
    "start": "babel src/lib -d lib --watch",
    "prebuild": "rimraf build",
    "build": "babel src/lib -d lib",
    "watch": "npm run build -- --watch",
    "predoc": "rimraf doc",
    "doc": "esdoc -c ./esdoc.json",
    "prerelease": "npm run build",
    "release": "npm version patch && npm publish",
    "postrelease": "npm run release-doc",
    "prerelease-doc": "npm run doc",
    "release-doc": "git subtree push --prefix doc origin gh-pages",
    "postrelease-doc": "git commit -am \"doc-release\" && git push --follow-tags",
    "upgrade": "ncu -a && npm update",
    "_pretest": "npm run build",
    "test": "jest",
    "testw": "npm test -- --watch",
    "dev": "run-p testw watch"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "*",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-react": "*",
    "faker": "^3.1.0",
    "jest-cli": "*",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "redux-grid-view": "*",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "babel-core": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "chai": "^3.5.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "npm-run-all": "^2.1.1",
    "redux-grid-view": "*"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "fixed-data-table": "*"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "doc"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/fixed-data-table/",
      "<rootDir>/node_modules/faker/"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchamberlain/redux-grid.git"
  },
  "license": "MIT",
  "author": "Cole Chamberlain <cole.chamberlain@gmail.com> (https://github.com/cchamberlain)",
  "bugs": {
    "url": "https://github.com/cchamberlain/redux-grid/issues"
  },
  "homepage": "https://cchamberlain.github.io/redux-grid"
}

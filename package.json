{
  "name": "redux-grid",
  "version": "0.1.2",
  "description": "Grid component built for Redux.",
  "keywords": [
    "redux",
    "react",
    "grid",
    "tables",
    "charts",
    "charting",
    "pagination",
    "sort"
  ],
  "main": "lib/index.js",
  "scripts": {
    "link-dev": "npm link ../redux-grid-view",
    "start": "babel src/lib -d lib --watch",
    "prebuild": "rimraf build",
    "build": "babel src/lib -d lib",
    "predoc": "rimraf doc",
    "doc": "esdoc -c ./esdoc.json",
    "prerelease": "npm run build",
    "release": "npm version patch && npm publish",
    "postrelease": "npm run release-doc",
    "prerelease-doc": "npm run doc",
    "release-doc": "git subtree push --prefix doc origin gh-pages",
    "postrelease-doc": "git commit -am \"doc-release\" && git push --follow-tags",
    "upgrade": "ncu -a && npm update",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "redux-grid-view": "*",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.0.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "esdoc-es7-plugin": "0.0.3",
    "redux-grid-view": "*",
    "lodash.assign": "^4.0.8"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchamberlain/redux-grid.git"
  },
  "license": "MIT",
  "author": "Cole Chamberlain <cole.chamberlain@gmail.com> (https://github.com/cchamberlain)",
  "bugs": {
    "url": "https://github.com/cchamberlain/redux-grid/issues"
  },
  "homepage": "https://cchamberlain.github.io/redux-grid"
}
